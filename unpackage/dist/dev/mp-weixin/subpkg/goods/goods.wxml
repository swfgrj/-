<view><view class="good-item"><swiper class="swiper-container" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{2000}}" duration="{{1000}}" circular="{{true}}"><block wx:for="{{content.photo}}" wx:for-item="item" wx:for-index="index"><swiper-item><image style="width:100%;height:100%;" src="{{item}}" mode="aspectFill"></image></swiper-item></block></swiper><view class="box1"><view class="box1-son1"><view class="price">{{"￥"+content.price}}</view><view class="self-info"><image class="img" src="{{content.avatarUrl}}" data-openid="{{content._openid}}" data-event-opts="{{[['tap',[['gotoMyPublish',['$event']]]]]}}" bindtap="__e"></image><text class="text">{{content.nickName}}</text></view></view><view class="box1-son2"><input type="text" placeholder="{{content.title}}" placeholder-class="ph" disabled="true"/><text style="text-overflow:ellipsis;font-size:17px;">{{content.note}}</text></view></view><view class="box2"><view class="good-detail"><input type="text" placeholder="物品详情" placeholder-class="ph" disabled="true"/><textarea class="text">{{content.detail}}</textarea></view><view class="location"><image style="width:30px;height:30px;margin-right:15px;" src="../../static/location.png"></image><text>{{content.location}}</text></view></view></view><view class="bottom-box"><view data-event-opts="{{[['tap',[['collectGood',['$event']]]]]}}" class="save" bindtap="__e"><image class="img" src="{{isCollected===true?'../../static/collect.png':'../../static/uncollect.png'}}"></image><text>收藏</text></view><view data-event-opts="{{[['tap',[['chatWithOwner',['$event']]]]]}}" class="communication" bindtap="__e"><image class="img" src="../../static/communication.png"></image><text>沟通</text></view></view></view>